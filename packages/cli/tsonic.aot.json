{
  "$schema": "https://tsonic.org/schema/v1.json",
  "rootNamespace": "Webpods.CLI",
  "entryPoint": "src/app.ts",
  "sourceRoot": "src",
  "outputDirectory": "generated",
  "outputName": "webpods-aot",
  "output": {
    "nativeAot": true,
    "trimmed": true,
    "singleFile": true
  },
  "buildOptions": {
    "stripSymbols": true,
    "invariantGlobalization": true
  },
  "dotnet": {
    "typeRoots": ["../../node_modules/@tsonic/globals"],
    "libraries": ["../commands/dist/net10.0/Webpods.Commands.dll", "lib/Tsonic.JSRuntime.dll"]
  }
}
