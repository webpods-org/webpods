{
  "$schema": "https://tsonic.org/schema/v1.json",
  "rootNamespace": "Webpods.Commands",
  "entryPoint": "src/index.ts",
  "sourceRoot": "src",
  "outputDirectory": "generated",
  "outputName": "Webpods.Commands",
  "output": {
    "type": "library",
    "targetFrameworks": [
      "net10.0"
    ],
    "generateDocumentation": false,
    "includeSymbols": false,
    "packable": false
  },
  "buildOptions": {
    "stripSymbols": true,
    "invariantGlobalization": true
  },
  "references": {
    "libraries": ["../ssh/dist/net10.0/Webpods.SSH.dll"]
  }
}
